        "import math\n",
        "\n",
        "def deret_bilangan(n):\n",
        "    # hasil = 0\n",
        "    for i in range(n, 0, -1):\n",
        "        hasil = math.factorial(i)\n",
        "        print(hasil, end=\" \")\n",
        "        for j in range(i,0,-1):\n",
        "            print(j,end=' ')\n",
        "        print()\n",
        "\n",
        "deret_bilangan(6)"
      ]
